datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}



model Job {
  id            String    @id @default(auto()) @map("_id") @db.ObjectId
  user_Id        String
  title         String
  description   String
  orgName       String?
  remote        String
  
 
}